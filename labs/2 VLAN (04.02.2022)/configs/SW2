conf t
hostname SW2
no ip domain-lookup
enable secret class
line console 0
password cisco
login
exit
line vty 0 4
password cisco
login
exit
service password-encryption
banner login Unathorization access deny
vlan 3
name Managment
vlan 7
name ParkingLot
vlan 8
name Native
exit
interface vlan 3
ip address 192.168.3.12 255.255.255.0
no shutdown
exit
ip default-gateway 192.168.3.1
interface range e0/2-3
switchport mode access
switchport access vlan 7
shutdown
exit
interface e0/1
switchport mode access 
switchport access vlan 3
exit
